<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- RedmineURL -->
    <add key="RedmineUrl" value="http://localhost:3000" />
    <!-- RedmineAPIKey -->
    <add key="RedmineApiKey" value="9b3572bf4f2cccdcdd0c254371a38babeb1004c7" />
    <!-- Prefix for ticket IDs in email subjects. subject ex)[id:10001] your billing status requere here. -->
    <add key="idprefix" value="id:" />
    <!-- Delimiters for detecting quoted text in email replies  -->
    <add key="ReplyDelimiter1" value="^On .+ wrote:" />
    <add key="ReplyDelimiter2" value="^From: .+" />
    <add key="ReplyDelimiter3" value="^-----Original Message-----" />
    <add key="ReplyDelimiter4" value="^\d{4}年\d{1,2}月\d{1,2}日\(.+\) \d{1,2}:\d{2} .+ .+@.+\..+:" />
  </appSettings>
  
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <!-- logfile path -->
        <add 
          name="fileListener" 
          type="System.Diagnostics.TextWriterTraceListener" 
          initializeData="%TEMP%\CrmOutlookAddIn.log" />
        <remove name="Default" />
      </listeners>
    </trace>
  </system.diagnostics>
  
